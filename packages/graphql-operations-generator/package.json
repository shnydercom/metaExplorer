{
  "name": "@metaexplorer/graphql-operations-generator",
  "version": "0.0.12",
  "description": "graphql-generator for metaexplorer projects",
  "author": "Jonathan Schneider",
  "license": "EUPL-1.2",
  "scripts": {
    "lint": "tslint src/**/*.ts",
    "prepack": "bob-update-version",
    "build-generator": "bob",
    "test": "jest --no-watchman --config ../../jest.config.js",
    "watch": "jest --watch --config ../../jest.config.js"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "1.15.3",
    "@graphql-codegen/visitor-plugin-common": "1.15.3",
    "@metaexplorer/core": "^0.0.23",
    "auto-bind": "4.0.0",
    "tslib": "1.11.1"
  },
  "devDependencies": {
    "@ardatan/bob": "0.2.7",
    "@graphql-codegen/testing": "1.15.3",
    "graphql": "14.6.0",
    "jest": "25.1.0",
    "jest-docblock": "25.1.0",
    "jest-junit": "10.0.0",
    "ts-jest": "25.2.1",
    "typescript": "3.8.3"
  },
  "peerDependencies": {
    "graphql": "^0.12.0 || ^0.13.0 || ^14.0.0"
  },
  "sideEffects": false,
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "jest-junit": {
    "outputDirectory": "./test-results/typescript"
  },
  "private": true
}
